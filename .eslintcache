 \n  return (\n    <Switch>\n        <Route exact path={PUBLIC_ROUTES.map((route) => route.path)}>\n        <PublicLayout>\n          <RenderRoutes routes={PUBLIC_ROUTES} />\n        </PublicLayout>\n      </Route>\n\n        <Route path={PRIVATE_ROUTES.map((route) => route.path)}>\n        <PrivateLayout>\n          <RenderRoutes routes={PRIVATE_ROUTES} />\n       </PrivateLayout>\n        </Route>\n     \n     <Redirect from=\"*\" to={DEFAULT_AUTHENTICATED_ROUTE} />\n    \n      </Switch>\n  );\n};\n\nconst RootRouter = () => {\n\n const dispatch=useDispatch()\n  useEffect(() => {\n    dispatch(getData([]));\n  }, [])\n\n  const token = useSelector((state) => state.data.token);\n  const baseName = process.env.REACT_APP_BASE_NAME;\n  const isAuthenticated = !!token;\n  return (\n\n\n    <BrowserRouter basename={baseName}>\n      <DocumentTitle isAuthenticated={isAuthenticated} />\n      <AppLayout isAuthenticated={isAuthenticated}>{token? <AuthenticatedRoutes /> : <GuestRoutes />}</AppLayout>\n    </BrowserRouter>\n\n  );\n};\n\nexport default RootRouter;\n","/home/chicmic/Documents/React-JavaScript-Study/CricketApp-1/src/Redux/loginStore.js",[],"/home/chicmic/Documents/React-JavaScript-Study/CricketApp-1/src/Redux/Reducers/RootReducer.js",[],"/home/chicmic/Documents/React-JavaScript-Study/CricketApp-1/src/Redux/Sagas/RootSaga.js",["212"],"import { all, fork } from \"redux-saga/effects\";\n\nimport watchAuth from \"./Auth\";\nimport loginSaga from \"./loginSaga\";\n\nfunction* rootSaga() {\n  yield all([fork(watchAuth)]);\n}\n\nexport default rootSaga;\n","/home/chicmic/Documents/React-JavaScript-Study/CricketApp-1/src/Routes/PublicRoutes.js",[],"/home/chicmic/Documents/React-JavaScript-Study/CricketApp-1/src/Routes/RenderRoutes.js",[],"/home/chicmic/Documents/React-JavaScript-Study/CricketApp-1/src/Routes/DocumentTitle.js",[],"/home/chicmic/Documents/React-JavaScript-Study/CricketApp-1/src/Routes/AuthRoutes.js",[],"/home/chicmic/Documents/React-JavaScript-Study/Cr