ation = ({showperPage,onPaginationChanges,total}) => {\n    const[counter,setCounter]=useState(1);\n    \n    const onButtonClick=(type)=>{\n        if(type==='prev'){\n            if(counter===1){\n                setCounter(1);\n            }\n            else {\n                \n                setCounter(counter-1);\n            }\n        }\n        else if(type==='next'){\n            if(Math.ceil(total/showperPage)===counter||total==0){\n                setCounter(counter);\n            }\n            else{\n                setCounter(counter+1);\n                \n            }\n        }\n    }\n\nuseEffect(()=>{\n    const value =showperPage*counter;\n\n    onPaginationChanges(value-showperPage,value);\n\n},[counter])\n  return (\n    <>\n    \n    <div className='p-5 d-flex justify-content-center w-100'>\n      {counter>1?<button className='btn btn-outline-primary px-5' onClick={()=>{onButtonClick('prev')}}>Back</button>:null}\n    <label className=\"mx-5 my-2 \">{counter}</label>\n    {total>4&&(Math.ceil(total/11))!==counter?<button className='btn btn-outline-primary px-5' onClick={()=>{onButtonClick('next')}}>Next</button>:null}\n    </div>\n    </>\n\n   \n    \n  )\n\n}\n\nexport default Pagination","/home/chicmic/Documents/React-JavaScript-Study/CricketApp-1/src/Components/Atoms/customInput/index.js",[],"/home/chicmic/Documents/React-JavaScript-Study/CricketApp-1/src/Components/Atoms/customRadio/index.js",[],"/home/chicmic/Documents/React-JavaScript-Study/CricketApp-1/src/Components/Atoms/TableButton/index.js",["228"],{"ruleId":"229","replacedBy":"230"},{"ruleId":"231","replacedBy":"232"},{"ruleId":"233","severity":1,"message":"234","line":2,"column":10,"nodeType":"235","messageId":"236","endLine":2,"endColumn":18},{"ruleId":"233","severity":1,"message":"237","line":3,"column":50,"nodeType":"235","messageId":"236","endLine":3,"endColumn":54},{"ruleId":"238","severity":1,"message":"239","line":60,"column":6,"nodeType":"240","endLine":60,"endColumn":